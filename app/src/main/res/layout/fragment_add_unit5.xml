<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/tema"
    tools:context="com.example.azizainun.maps.AddUnit5">

    <!-- TODO: Update blank fragment layout -->
    <com.example.azizainun.maps.MyTextView
        android:id="@+id/text"
        android:layout_width="match_parent"
        android:layout_height="128sp"
        android:layout_margin="15sp"
        android:layout_marginTop="25sp"
        android:textSize="32.36sp"
        android:text="Dimanakah letak tempat yang akan disewakan?"/>
    <com.google.android.gms.maps.MapView
        android:layout_width="match_parent"
        android:layout_height="200sp"
        android:id="@+id/map_2"
        android:clickable="false"
        map:liteMode="true"
        android:layout_below="@+id/text"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <com.example.azizainun.maps.MyTextView
        android:id="@+id/kota"
        android:layout_width="match_parent"
        android:layout_height="128sp"
        android:layout_below="@+id/map_2"
        android:textSize="32.36sp"
        android:layout_margin="15sp"
        android:layout_marginTop="25sp"
        android:text="Kota/kabupaten dari tempat yang disewakan"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        android:layout_below="@+id/kota"/>
    <!--<fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        android:name="com.google.android.gms.maps.MapFragment"
        android:id="@+id/map_1"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        map:cameraZoom="13"
        map:mapType="normal"
        android:layout_below="@+id/text"
        map:liteMode="true"/>-->
</RelativeLayout>
